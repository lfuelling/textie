<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
    <RandomAccessFile name="File" fileName="logs/textie.log">
      <PatternLayout>
        <Pattern>%d [%p] %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </RandomAccessFile>
  </Appenders>
  <Loggers>
    <Root level="TRACE">
      <AppenderRef ref="File"/>
    </Root>
  </Loggers>
</Configuration>